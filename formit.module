<?php

/**
 * @file
 * Contains formit.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_theme().
 */
function formit_theme() {
  
  $theme['formit_custom_form_render'] = [
    'variables' => ['first_name' => NULL, 'last_name' =>  NULL, 'biography' =>  NULL],
    'template' => 'formit_custom_form_render'
  ];
  return $theme;
}